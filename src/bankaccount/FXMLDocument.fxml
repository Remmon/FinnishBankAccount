<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="bankaccount.FXMLDocumentController">
    <children>
        <Button fx:id="convertBankNumberButton" layoutX="241.0" layoutY="14.0" onAction="#convertBankNumberButtonAction" text="Convert" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <TextField fx:id="smallBankNumberField" layoutX="14.0" layoutY="14.0" onAction="#convertBankNumberButtonAction" prefHeight="25.0" prefWidth="217.0" promptText="Bank Number in short format" />
      <Label layoutX="14.0" layoutY="83.0" prefHeight="17.0" prefWidth="217.0" text="Bank number in long format" />
      <Label fx:id="checkDigitCorrectLabel" layoutX="14.0" layoutY="129.0" text="CheckDigit:" />
      <Label fx:id="checkDigitCorrectLabel" layoutX="135.0" layoutY="146.0" text="Legit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="longBankNumberLabel" blendMode="DARKEN" layoutX="14.0" layoutY="100.0" text="Label" />
      <Label fx:id="checkDigitNumberLabel" layoutX="136.0" layoutY="129.0" text="Label" />
      <Label layoutX="14.0" layoutY="146.0" prefHeight="17.0" prefWidth="121.0" text="Is bank number legit?" />
      <Label fx:id="shortBankNumberFormatLabel" layoutX="16.0" layoutY="39.0" />
    </children>
</AnchorPane>
